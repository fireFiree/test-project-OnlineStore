{
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "webix": true,
        "$$": true
    },
    "extends": "./node_modules/eslint-xbsoftware/1__double_quotes_and_tabs/.eslintrc",
    "rules": {
        "no-extra-parens": "warn",
        "valid-jsdoc": "warn",
        "class-methods-use-this": "off",
        "no-param-reassign": "off",
        "no-unused-expressions": "off",
		"no-debugger": "off",
		"func-names": "off",
		"object-shorthand": "off",
		"eqeqeq": "off",
		"no-var": "off"
    },
	"plugins":[
		"import"
	]
}
